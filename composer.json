{
  "require": {
    "am-impact/amcommand": "^3.1",
    "craftcms/cms": "3.6.16",
    "craftcms/redactor": "^2.8",
    "deployer/deployer": "^6.8",
    "ether/seo": "^3.7",
    "nystudio107/craft-emptycoalesce": "^1.0",
    "nystudio107/craft-vite": "1.0.7",
    "sebastianlenz/linkfield": "^1.0",
    "studioespresso/craft-dumper": "^2.0",
    "verbb/expanded-singles": "^1.1",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}

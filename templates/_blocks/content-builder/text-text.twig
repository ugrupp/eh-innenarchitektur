{% embed "_components/section" with {
  self: self,
  options: {
    id: "#{ self.type|kebab }-#{ self.id }"
  }
} only %}
  {% block content %}
    {% embed "_components/container" with { self: self } only %}
      {% block content %}
        {% include "_components/text-text" with {
          options: {
            text1: self.text1,
            text2: self.text2,
          }
        } only %}
      {% endblock %}
    {% endembed %}
  {% endblock %}
{% endembed %}
